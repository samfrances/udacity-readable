{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "no-consecutive-blank-lines": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "arrow-parens": [true, "ban-single-arg-parens"],
        "callable-types": false,
        "trailing-comma": [
            true, {
                "singleline": "never",
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "ignore",
                    "typeLiterals": "always",
                    "imports": "always",
                    "exports": "always"
                }
            }
        ]
    },
    "rulesDirectory": []
}
